    "#A4-2(1 point)\n",
    "from positional_list import PositionalList \n",
    "class personal_PL_2(PositionalList):\n",
    "    def __reversed__(self):\n",
    "        cursor = self.last()\n",
    "        while cursor is not None:\n",
    "            yield cursor.element()\n",
    "            cursor = self.before(cursor)"
   ]
